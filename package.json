{
  "name": "webpack-4",
  "version": "1.0.0",
  "description": "curso de webpack 4",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multy:entries": "webpack --config ./clase-multy-entries/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugin": "webpack --config ./clase_plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./clase_dev_server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./clase_babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./clase_react/webpack.config.js",
    "build:url-loader": "webpack-dev-server --config ./clase_url_loader/webpack.config.js",
    "build:prepro": "webpack-dev-server --config ./clase_prepro/webpack.config.js"
  },
  "keywords": [
    "JS",
    "webpack"
  ],
  "author": "Carlos V.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "babel-loader": "8.1.0",
    "css-loader": "4.3.0",
    "file-loader": "6.1.0",
    "html-webpack-plugin": "4.5.0",
    "less": "3.12.2",
    "less-loader": "7.0.1",
    "mini-css-extract-plugin": "0.11.2",
    "node-sass": "4.14.1",
    "postcss-loader": "4.0.2",
    "postcss-nested": "5.0.1",
    "sass-loader": "10.0.2",
    "style-loader": "1.2.1",
    "stylus": "0.54.8",
    "stylus-loader": "3.0.2",
    "url-loader": "4.1.0",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
